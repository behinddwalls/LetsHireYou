<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans     
        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-4.0.xsd
        http://www.springframework.org/schema/mvc
        http://www.springframework.org/schema/mvc/spring-mvc-4.0.xsd
        http://www.springframework.org/schema/util 
        http://www.springframework.org/schema/util/spring-util-4.0.xsd">

	<!-- Api Call Configs -->
	<util:map id="apiCall">
		<entry key="host" value="www.evenrank.com" />
		<entry key="port" value="80" />
		<entry key="protocol" value="http" />
	</util:map>

	<util:map id="mySqlMapApi">
		<entry key="path" value="/api/maptosql" />
	</util:map>

	<util:map id="apiCallCredentials">
		<entry key="username" value="job-portal" />
		<entry key="password" value="job-portal123!@#" />
	</util:map>

</beans>